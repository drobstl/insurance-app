{
  "expo": {
    "name": "AgentForLife",
    "slug": "agentforlife",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "agentforlife",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "privacyPolicyUrl": "https://agentforlife.app/privacy",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.danielroberts.agentforlife",
      "buildNumber": "4",
      "infoPlist": {
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSContactsUsageDescription": "This app needs access to your contacts to send referrals to friends and family.",
        "NSPhotoLibraryUsageDescription": "This app does not access your photos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.danielroberts.agentforlife",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D4D4D"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D4D4D"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D4D4D"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "This app needs access to your contacts to send referrals to friends and family."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6765db0c-dbc2-48e1-8082-ec16e721c096"
      }
    },
    "owner": "danielroberts"
  }
}
