{
  "expo": {
    "name": "AgentForLife",
    "slug": "agentforlife",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "agentforlife",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "privacyPolicyUrl": "https://agentforlife.app/privacy",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danielroberts.agentforlife",
      "buildNumber": "16",
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSContactsUsageDescription": "This app needs access to your contacts to send referrals to friends and family.",
        "NSPhotoLibraryUsageDescription": "This app does not access your photos.",
        "NSUserNotificationsUsageDescription": "Your insurance agent may send you important updates about your policies, birthday greetings, and holiday messages.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIDeviceFamily": [
          1,
          2
        ],
        "UIRequiresFullScreen": true,
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "package": "com.danielroberts.agentforlife",
      "versionCode": 16,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D4D4D"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D4D4D"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D4D4D"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "This app needs access to your contacts to send referrals to friends and family."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3DD6C3"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6765db0c-dbc2-48e1-8082-ec16e721c096"
      }
    },
    "owner": "danielroberts"
  }
}
